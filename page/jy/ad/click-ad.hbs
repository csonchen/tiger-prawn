<table class="table table-bordered table-hover" data-ad_id="{{id}}" data-start="{{start}}" data-end="{{end}}" data-mediator-class="tp.page.ad.ClickSingleAd" data-url="{{API}}click/{{id}}" data-ranger=".date-range" data-defaults="id={{id}}" data-id-attribute="date">
    <thead>
    <tr>
        <th>时间</th>
        <th>下单数量</th>
        <th>实际跑量</th>
        <th>单价（元）</th>
        <th>总价（元）</th>
    </tr>
    </thead>
    <tbody>
    <script type="text/x-handlebars-template">
        {{{{raw-helper}}}}
            {{#if is_amount}}
            <tr id="amount">
                <td>{{date}}</td>
                <td>{{quotes}}</td>
                <td>{{nums}}</td>
                <td>/</td>
                <td></td>
            </tr>
            {{else}}
            <tr id="{{date}}">
                <td>{{date}}</td>
                <td><input type="number" value="{{quotes}}" class="quotes form-control" tabindex="{{counter}}" {{#if disabled}}disabled{{/if}}/></td>
                <td><input type="number" value="{{nums}}" class="cpa form-control" tabindex="{{counter}}" {{#if disabled}}disabled{{/if}}/></td>
                <td><input type="number" value="{{quote_rmb}}" class="price form-control" tabindex="{{counter}}" {{#if disabled}}disabled{{/if}}/></td>
                <td></td>
            </tr>
            {{/if}}
        {{{{/raw-helper}}}}
    </script>
    </tbody>
    <tfoot class="waiting">
    <tr>
        <td colspan="5"><p align="center"><i class="fa fa-spinner fa-spin fa-2x"></i><br>加载中，请稍候</p></td>
    </tr>
    </tfoot>
</table>